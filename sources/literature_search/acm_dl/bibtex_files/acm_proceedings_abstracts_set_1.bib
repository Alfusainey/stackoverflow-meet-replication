abstract = {The development of academic functions of social media has brought a number of academic users to social media. Starting from social media, it is beneficial to promote the future development of social media and realize the sustainable development of academic exchanges to study the influencing factors of knowledge sharing behavior of academic users. Based on the Information System Success Model and Expectation confirmation theory, this paper obtained academic user data through questionnaire survey, and concluded that system quality and information quality have a positive impact on perceived usefulness, and perceived usefulness and expectation confirmation have a positive impact on academic users’ knowledge sharing behavioral intention.},
booktitle = {Artificial Intelligence and Security: 8th International Conference, ICAIS 2022, Qinghai, China, July 15–20, 2022, Proceedings, Part II},
pages = {253–263},
numpages = {11},
