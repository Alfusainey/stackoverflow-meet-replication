@inproceedings{10.1007/978-3-031-06788-4_22,
author = {Chen, Xiaoyu and Zhang, Shiqi and Fan, Xiaowen and Liu, Huan and Li, Yacong},
title = {Knowledge Sharing Behavior for Academic Users of Social Media and Its Empirical Research},
year = {2022},
