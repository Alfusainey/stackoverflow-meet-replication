@inproceedings{10.1145/3524842.3527966,
author = {Zimmerle, Carlos and Gama, Kiev and Castor, Fernando and Filho, Jos\'{e} Murilo Mota},
title = {Mining the usage of reactive programming APIs: a study on GitHub and stack overflow},
year = {2022},
isbn = {9781450393034},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3524842.3527966},
doi = {10.1145/3524842.3527966},
abstract = {Conventionally, callbacks and inversion of control have been the main tools to structure event-driven applications. Sadly, those patterns constitute a well-known source of design problems. The Reactive Programming (RP) paradigm has arisen as an approach to mitigate these problems. Yet, little evidence has been provided regarding the advantages of RP, and concerns have also arisen about the API usability of RP libraries given their disparate number of operators. In this work, we conduct a study on GitHub (GH) and Stack Overflow (SO) and explore three Reactive Extensions (Rx) libraries (RxJava, RxJS, and RxSwift) with the most GH projects to understand how much the vast Rx operators are being used. Also, we examine Rx SO posts to complement the results from the GH exploration by understanding the problems faced by RP developers and how they relate with the operators' frequencies found in open source projects. Results reveal that, in spite of its API size, the great majority of the Rx operators are actually being used (95.2\%), with only a few, mostly related to RxJava, not being utilized. Also, we unveil 23 topics from SO with more posts concerning the Stream Abstraction (36.4\%). Posts related to Dependency Management, Introductory Questions, and iOS Development figure as relevant topics to the community. The findings herein present can not only stimulate advancements in the field by understanding the usage of RP API and the main problems faced by developers, but also help newcomers in identifying the most important operators and the areas that are the most likely to be relevant for a RP application.},
booktitle = {Proceedings of the 19th International Conference on Mining Software Repositories},
pages = {203–214},
numpages = {12},
keywords = {API usability, mining software repositories, reactive programming},
location = {Pittsburgh, Pennsylvania},
series = {MSR '22}
}

@inproceedings{10.5555/2452580.2455892,
author = {Junwei, Duan and Qiming, Dong},
title = {Study on Development of Industry Clusters Based on Knowledge Management},
year = {2012},
isbn = {9780769547442},
publisher = {IEEE Computer Society},
address = {USA},
abstract = {Recently, the world economy has been quietly transformed from the Industry economy time to the knowledge economy time. The effective knowledge Management innovation method causes knowledge to become the first Propelling force of economy. And it impels the enterprises to grow continually which brings them to succeed in the intense market Competition. Therefore as one of the unique but also infinite resource the knowledge has already become the core essential factor of the Economic development. Moreover looks over the new world economy especially the European and American economies development histories we may discover that the Industry Clusters has Played the extremely Role to the development of their economy. This paper mainly discusses the characteristics of knowledge management and knowledge sharing in industry clusters, structure of knowledge sharing in industry cluster, then gives an evaluation about knowledge sharing process. Knowledge sharing evaluation services to enable enterprises to know your role in the cluster, as well as for the enterprises within the cluster to better share the knowledge relevant enterprises to provide reference and basis.},
booktitle = {Proceedings of the 2012 3rd International Conference on E-Business and E-Government - Volume 05},
pages = {1051–1054},
