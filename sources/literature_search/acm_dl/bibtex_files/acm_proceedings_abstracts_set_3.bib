@inproceedings{10.1007/11600930_83,
author = {Zhang, Lingling and Li, Jun and Shi, Yong},
title = {Study on improving efficiency of knowledge sharing in knowledge-intensive organization},
year = {2005},
isbn = {3540309004},
publisher = {Springer-Verlag},
address = {Berlin, Heidelberg},
url = {https://doi.org/10.1007/11600930_83},
doi = {10.1007/11600930_83},
abstract = {Knowledge sharing is an important part in knowledge management. The value of knowledge only can be seen in its transfer, sharing and utilizing. The transfer of individual tacit knowledge to organizational capacity can improve the competitiveness of a organization. However, there are a lot of barriers in knowledge sharing, especially in those knowledge-intensive organizations, where knowledge is very important for individuals to keep advantage, so they are usually unwilling to share it with others, or ‘contribute’ their personal knowledge to the organization. At present, most studies are emphasizing on how to create a good physical environment and platform to improve it. In this paper, we’ll analyze it using game theory. We think that to improve the efficiency of knowledge sharing efficiency, motivation mechanism and physical platform must be improved simultaneously, especially in knowledge-intensive organization, motivation mechanism is more important. Then, to problem of ‘prisoner’ dilemma’ between individuals knowledge sharing, this paper will show how to design motivation method to improve efficiency of it.},
booktitle = {Proceedings of the First International Conference on Internet and Network Economics},
pages = {816–825},
numpages = {10},
keywords = {efficiency of knowledge sharing, game theory, knowledge-intensive organization},
location = {Hong Kong, China},
series = {WINE'05}
}

@inproceedings{10.1109/ICIII.2011.79,
author = {Zhiying, Ji},
title = {Evolutionary Game Analysis of Knowledge Sharing among Integrated Innovation Network (IIN) Members},
year = {2011},
isbn = {9780769545233},
publisher = {IEEE Computer Society},
address = {USA},
url = {https://doi.org/10.1109/ICIII.2011.79},
doi = {10.1109/ICIII.2011.79},
abstract = {This paper analyzes integrated innovation network co-operation between enterprises based on innovative knowledge sharing behavior with evolutionary game model. Through theoretical analysis, it is found that coordination coefficient, penalty coefficient, the cost of knowledge transfer, innovation gains and the shares in the innovation gains of members and so on will affect the success of cooperation and innovation between companies. This paper seeks to give some policy based on the above conclusions.},
booktitle = {Proceedings of the 2011 International Conference on Information Management, Innovation Management and Industrial Engineering - Volume 01},
