@inproceedings{10.1145/2018431.2018434,
author = {Engel, Juergen and Maertin, Christian and Herdin, Christian},
title = {Pattern-based user interface transformation for knowledge sharing applications},
year = {2011},
isbn = {9781450308564},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/2018431.2018434},
doi = {10.1145/2018431.2018434},
abstract = {In this paper we present a pattern-based method for transforming user interfaces of knowledge sharing applications. We highlight a hierarchical pattern language which has been developed in the context of project p.i.t.c.h. (pattern-based interactive tools for improved communication habits in knowledge transfers). On this foundation we use the transformation process of the PaMGIS (Pattern-based Modeling and Generation of Interactive Systems) framework in order to create user interfaces for various contexts of use.},
booktitle = {Proceedings of the 2nd International Workshop on Pattern-Driven Engineering of Interactive Computing Systems},
pages = {8–11},
numpages = {4},
keywords = {HCI patterns, knowledge sharing application, pattern language, user interface transformation},
