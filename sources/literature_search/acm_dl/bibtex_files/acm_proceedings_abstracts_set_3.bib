@inproceedings{10.1109/CGC.2012.99,
author = {Jiang, Fan and Leung, Carson Kai-Sang and Tanbeer, Syed K.},
title = {Finding Popular Friends in Social Networks},
year = {2012},
isbn = {9780769548647},
publisher = {IEEE Computer Society},
address = {USA},
url = {https://doi.org/10.1109/CGC.2012.99},
doi = {10.1109/CGC.2012.99},
abstract = {The emergence of social computing enables users to intersect social behaviour with computing systems and to create social conventions as well as social contexts through the use of software and technology. Social networking sites have become popular to facilitate collaboration and knowledge sharing between users. A rich set of information is embedded in these social media data. In this paper, we propose a data mining algorithm to help users to find popular friends in social networks. This can be considered as an application of data mining and social computing.},
