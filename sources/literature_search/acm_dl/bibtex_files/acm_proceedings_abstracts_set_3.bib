}

@inproceedings{10.1007/978-3-642-02774-1_12,
author = {Razali, Suriyati and Md. Noor, Nor Laila and Wan Adnan, Wan Adilah},
title = {Structuring the Social Subsystem Components of the Community Based E-Museum Framework},
